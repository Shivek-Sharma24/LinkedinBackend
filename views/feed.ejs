<% include ./partials/header.ejs  %>
<div class="cards flex flex-wrap gap-10 px-10   bg-zinc-700  w-full min-h-screen text-white">
<% posts.forEach(function(post , index){ %>
  <div class="card mt-5">
    
    <div class="bg-zinc-200 h-40 w-52 rounded-md overflow-hidden">
      <img src="/images/<%=post.postimage %>" alt="" class=" object-cover  w-full h-full">
    </div>
    <h3 class="text-xl font-semibold mt-3"><%= post.title %></h3>
    <h5 class="font-semibold text-lg opacity-60">Post by <%= post.user.username %> </h5>
    </div>
<% }) %>
</div>


<% include ./partials/footer.ejs  %>